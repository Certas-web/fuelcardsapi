<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\ApiUsers" table="api_users">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="name" type="string" column="name" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="email" type="string" column="email" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="business" type="string" column="business" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="jobTitle" type="string" column="job_title" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="contactNo" type="string" column="contact_no" length="200" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
