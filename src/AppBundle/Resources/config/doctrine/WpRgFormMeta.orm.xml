<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\WpRgFormMeta" table="wp_rg_form_meta">
    <id name="formId" type="integer" column="form_id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="displayMeta" type="text" column="display_meta" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="entriesGridMeta" type="text" column="entries_grid_meta" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="confirmations" type="text" column="confirmations" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="notifications" type="text" column="notifications" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
