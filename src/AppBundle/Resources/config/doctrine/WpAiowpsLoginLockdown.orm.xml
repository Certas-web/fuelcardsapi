<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\WpAiowpsLoginLockdown" table="wp_aiowps_login_lockdown">
    <id name="id" type="bigint" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="userId" type="bigint" column="user_id" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="userLogin" type="string" column="user_login" length="150" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="lockdownDate" type="datetime" column="lockdown_date" nullable="false">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="releaseDate" type="datetime" column="release_date" nullable="false">
      <options>
        <option name="default">0000-00-00 00:00:00</option>
      </options>
    </field>
    <field name="failedLoginIp" type="string" column="failed_login_ip" length="100" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="lockReason" type="string" column="lock_reason" length="128" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
    <field name="unlockKey" type="string" column="unlock_key" length="128" nullable="false">
      <options>
        <option name="fixed"/>
        <option name="default"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
